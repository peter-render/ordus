  object qryOrderEdit: TFDQuery
    MasterSource = frmOrderLista.dsoOrderlist
    MasterFields = 'OrderID'
    Connection = dm.FDConnection1
    SQL.Strings = (
      'Select  o.Id , o.KundId,Ordernummer,Orderdatum,k.Kundnamn,'
      
        'Godsm'#228'rke,ReferensId,OrdertypId,OrderStatusId,Leveransdatum,Fakt' +
        'uranummer,Fakturadatum,Fritext,'
      'YtbehandlingDatum,YtbehandlingId'
      'from OrderHuvud o '
      'left join kund k on k.id = o.Kundid'
      'where o.Id = :OrderID')
    Left = 360
    Top = 52
