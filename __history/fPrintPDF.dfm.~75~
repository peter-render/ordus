  object qry: TFDQuery
    OnCalcFields = qryCalcFields
    ConstraintsEnabled = True
    MasterFields = 'OrderID'
    Connection = dm.FDConnection1
    SQL.Strings = (
      'Select '
      'o.Id OrderhuvudId,'
      'o.Godsm'#228'rke,'
      'orad.Id OrderradId,'
      'a.Artikelnummer,'
      'o.LeveransDatum,'
      'o.YtbehandlingDatum,'
      'orad.Antal,'
      'a.PDFFilnamn'
      ''
      'from orderrad orad'
      'join Orderhuvud o on o.id = Orad.OrderId'
      'join Artikel a on a.id = orad.ArtikelID'
      'where orad.OrderID = :OrderID'
      'order by orad.radnr'
      '')
    Left = 280
    Top = 232
    ParamData = <
      item
        Name = 'ORDERID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
