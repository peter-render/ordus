  object qryOrd: TFDQuery
    MasterSource = frmShowOrder.dsoQry
    Connection = dm.FDConnection1
    SQL.Strings = (
      ''
      'Select '
      #9'a.Id ArtikelId,'
      #9'a.Artikelnummer,'
      #9'a.Beteckning ArtikelBeteckning,'
      #9'a.KundID,'
      '  a.Lagerplats,'
      '  a.Lagersaldo,'
      '  k.Kundnamn,'
      '  Orderrad.Id OrderradId,'
      #9'orderrad.PrisperEnhet ,'
      #9'orderrad.Antal,'
      '  convert (char(10),orderrad.datum,120) Kortdatum ,'
      '  pit.Beteckning Prisdel,'
      #9'orp.Belopp OrderradPrisdelBelopp,'
      #9'y.Beteckning YtbehandlingBeteckning,'
      'o.OrderNummer,'
      'o.Id OrdusNummer,'
      'p.F'#246'rnamn'
      'from orderrad'
      'join artikel a on a.Id = orderrad.artikelID'
      'left join kund k on k.Id = a.KundID'
      'join orderhuvud o on o.ID = orderrad.orderID'
      'left Join Ytbehandling y on y.ID = orderrad.YtbehandlingID'
      'left join Orderradprisinfo orp on orp.OrderradID = orderrad.id'
      'left join Prisinfotyp pit on pit.Id = orp.PrisinfotypId'
      'left join personal p on p.id = orderrad.AvrapporteradPersonID'
      'where orderrad.artikelid = :ArtikelId'
      'order by orderrad.Id Desc ,orderrad.Datum asc')
    Left = 32
    Top = 92
    ParamData = <
      item
        Name = 'ARTIKELID'
        DataType = ftInteger
        ParamType = ptInput
        Value = Null
      end>
  end
end
