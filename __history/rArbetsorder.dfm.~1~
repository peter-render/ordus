  object qry: TADOQuery
    Connection = dm.ADOConnection1
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'OrderID'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      'Select *,Prisperenhet * Antal as Pris  from OrderRad orad'
      'join Artikel a on orad.artikelID = a.artikelid'
      'join OrderHuvud oh on oh.OrderID = orad.OrderID'
      'join kund k on k.Kundid = oh.kundid'
      'where oh.OrderID = :OrderID'
      'order by orad.Radnr'
      ' ')
    Left = 29
    Top = 77
    object qryOrderID: TIntegerField
      FieldName = 'OrderID'
    end
    object qryArtikelID: TIntegerField
      FieldName = 'ArtikelID'
    end
    object qryRadNr: TAutoIncField
      FieldName = 'RadNr'
      ReadOnly = True
    end
    object qryPositionnummer: TIntegerField
      FieldName = 'Positionnummer'
    end
    object qryAntal: TFloatField
      FieldName = 'Antal'
    end
    object qryPrisPerEnhet: TBCDField
      FieldName = 'PrisPerEnhet'
      DisplayFormat = '### ##0.00'
      Precision = 19
    end
    object qryDatum: TDateTimeField
      FieldName = 'Datum'
    end
    object qryArtikelnummer: TStringField
      FieldName = 'Artikelnummer'
      FixedChar = True
      Size = 30
    end
    object qryBeteckning: TStringField
      FieldName = 'Beteckning'
      Size = 200
    end
    object qryYtbehandlingIDdefault: TStringField
      FieldName = 'YtbehandlingIDdefault'
      FixedChar = True
      Size = 5
    end
    object qryKundID: TIntegerField
      FieldName = 'KundID'
    end
    object qryOrderNummer: TStringField
      FieldName = 'OrderNummer'
      Size = 30
    end
    object qryOrderdatum: TDateTimeField
      FieldName = 'Orderdatum'
    end
    object qryGodsmrke: TStringField
      FieldName = 'Godsm'#228'rke'
      Size = 50
    end
    object qryReferens: TStringField
      FieldName = 'Referens'
      Size = 50
    end
    object qryKundnamn: TStringField
      FieldName = 'Kundnamn'
      Size = 50
    end
    object qryPris: TFloatField
      FieldName = 'Pris'
      ReadOnly = True
      DisplayFormat = '### ##0.00'
    end
  end
